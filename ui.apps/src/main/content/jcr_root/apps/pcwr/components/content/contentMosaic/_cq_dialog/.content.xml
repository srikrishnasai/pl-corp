    jcr:primaryType="nt:unstructured"
    jcr:title="Content Mosaic"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <main
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Main"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                        	<column
                        		jcr:primaryType="nt:unstructured"
                        		sling:resourceType="granite/ui/components/coral/foundation/container">
                        		<items jcr:primaryType="nt:unstructured">
	                                <reduceTopPadding
	                                    jcr:primaryType="nt:unstructured"
	                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
	                                    name="./reduceTopPadding"
	                                    text="Connect to Component Above?"
	                                    value="true"/>		                                              
                                </items>
                                </column>                                
                        </items>
                    </main>
                    <card1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Card 1"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                        	<column
                        		jcr:primaryType="nt:unstructured"
                        		sling:resourceType="granite/ui/components/coral/foundation/container">
                        		<items jcr:primaryType="nt:unstructured">
		                            <card1Icon
		                                jcr:primaryType="nt:unstructured"
		                                sling:resourceType="pcwr/cq/gui/components/authoring/dialog/fileupload"
		                                autoStart="{Boolean}false"
		                                class="cq-droptarget"
		                                fieldLabel="Card Icon"
		                                fileNameParameter="./card1IconFileName"
		                                fileReferenceParameter="./card1IconFileRef"
		                                mimeTypes="[image/jpeg,image/png,image/svg]"
		                                multiple="{Boolean}false"
		                                name="./card1IconFile"
		                                title="Upload Image Asset"
		                                uploadUrl="${suffix.path}"
		                                useHTML5="{Boolean}true"
		                                allowUpload="{Boolean}false"/>                        		
		                            <card1Headline
		                                jcr:primaryType="nt:unstructured"
		                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
		                                fieldDescription="Please provide Headline for the Card"
		                                fieldLabel="Headline"
		                                name="./card1Headline"
		                                />
		                            <card1Body
		                                jcr:primaryType="nt:unstructured"
		                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
		                                fieldDescription="Please provide Text for the Card Body"
		                                fieldLabel="Card Body"
		                                name="./card1Body"
		                                />		                                
		                            <card1BgImg
		                                jcr:primaryType="nt:unstructured"
		                                sling:resourceType="pcwr/cq/gui/components/authoring/dialog/fileupload"
		                                autoStart="{Boolean}false"
		                                class="cq-droptarget"
		                                fieldLabel="Background Image"
		                                fileNameParameter="./card1BgImgFileName"
		                                fileReferenceParameter="./card1BgImgFileRef"
		                                mimeTypes="[image/jpeg,image/png]"
		                                multiple="{Boolean}false"
		                                name="./card1BgImgFile"
		                                title="Upload Image Asset"
		                                uploadUrl="${suffix.path}"
		                                useHTML5="{Boolean}true"
		                                allowUpload="{Boolean}false"/>
				                    <card1DisplayCTA
				                        jcr:primaryType="nt:unstructured"
				                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
				                        name="./card1DisplayCTA"
				                        text="Enable Call To Action"
				                        value="true"/>		                                
				                    <card1CTALabel
				                        jcr:primaryType="nt:unstructured"
				                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
				                        fieldDescription="Enter a Call To Action such as Learn More or Get Started"
				                        fieldLabel="Action Label"
				                        name="./card1CTALabel"/>                        
				                    <card1CTATarget
				                        jcr:primaryType="nt:unstructured"
				                        sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
				                        fieldLabel="Target"
				                        name="./card1CTATarget"
				                        rootPath="/content/pl-corp"/>		                                                  
                                    <videoCTA1
