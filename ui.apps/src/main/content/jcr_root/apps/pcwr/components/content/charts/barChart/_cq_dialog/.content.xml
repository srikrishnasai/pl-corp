															jcr:primaryType="nt:unstructured"
															text="Turquoise (#24c5cf)"
															value="#24c5cf"/>
														<deepBlue
															jcr:primaryType="nt:unstructured"
															text="Deep Blue (#004685)"
															value="#004685"/>
														<brightBlue
															jcr:primaryType="nt:unstructured"
															text="Bright Blue (#008ed7)"
															value="#008ed7"/>
														<lightGrey
															jcr:primaryType="nt:unstructured"
															text="Light Grey (#F2F2F2)"
															value="#F2F2F2"/>
														<mediumGrey
															jcr:primaryType="nt:unstructured"
															text="Medium Grey (#979797)"
															value="#979797"/>
														<darkGrey
															jcr:primaryType="nt:unstructured"
															text="Dark Grey (#5A5A5A)"
															value="#5A5A5A"/>
														<black
															jcr:primaryType="nt:unstructured"
															text="Black"
															value="#000"/>
														<off-black-bg
															jcr:primaryType="nt:unstructured"
															text="Off Black Bg (#222222)"
															value="#222"/>
														<gold
															jcr:primaryType="nt:unstructured"
															text="Gold (#FFA800)"
															value="#FFA800"/>
													</items>
												</barColor>
                                            </items>
                                        </field>
                                    </chart1Bars>
                                </items>
                        	</column>                                
                        </items>
